        <activity android:name="com.yuejiajun.BreakfastExpress.activity.ChangePwdActivity"></activity>
        <activity android:name="com.yuejiajun.BreakfastExpress.activity.SetPayPwdActivity"></activity>
        <activity
            android:name="com.yuejiajun.BreakfastExpress.activity.SetPayPwdActivity2"
